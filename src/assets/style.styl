@font-face 
  font-family 'sell-icon'
  src  url('/fonts/sell-icon.eot?ttduy7')
  src  url('/fonts/sell-icon.eot?ttduy7#iefix') format('embedded-opentype'),
    url('/fonts/sell-icon.ttf?ttduy7') format('truetype'),
    url('/fonts/sell-icon.woff?ttduy7') format('woff'),
    url('/fonts/sell-icon.svg?ttduy7#sell-icon') format('svg')
  font-weight normal
  font-style normal


[class^="icon-"], [class*=" icon-"] 
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family 'sell-icon' !important
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1

  /* Better Font Rendering =========== */
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale


.icon-search:before 
  content "\e900"

.icon-close:before 
  content "\e901"

.icon-address:before 
  content "\e902"

.icon-right:before 
  content "\e903"

.icon-user:before 
  content "\e904"

.icon-lock:before 
  content "\e905"

.icon-hand-shake:before 
  content "\e906"
